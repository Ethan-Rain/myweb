<mapper namespace="cn.helloworld1999.myweb.mapper.ImageMetadataTagsMapper">

    <resultMap id="BaseResultMap" type="cn.helloworld1999.myweb.demos.api.model.ImageMetadataTags">
            <id property="imageId" column="image_id" />
            <id property="tag" column="tag" />
    </resultMap>

    <sql id="Base_Column_List">
        image_id,tag
    </sql>

    <select id="selectByPrimaryKey" parameterType="map" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from image_metadata_tags
        where  image_id = #{imageId} AND tag = #{tag} 
    </select>

    <delete id="deleteByPrimaryKey" parameterType="map">
        delete from image_metadata_tags
        where  image_id = #{imageId} AND tag = #{tag} 
    </delete>
    <insert id="insert">
        insert into image_metadata_tags
        ( image_id,tag)
        values (#{imageId},#{tag})
    </insert>
    <insert id="insertSelective">
        insert into image_metadata_tags
        <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="imageId != null">image_id,</if>
                <if test="tag != null">tag,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="imageId != null">#{imageId},</if>
                <if test="tag != null">#{tag},</if>
        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="cn.helloworld1999.myweb.demos.api.model.ImageMetadataTags">
        update image_metadata_tags
        <set>
                <if test="tag != null">
                    tag = #{tag},
                </if>
        </set>
        where   image_id = #{imageId} AND tag = #{tag} 
    </update>
    <update id="updateByPrimaryKey" parameterType="cn.helloworld1999.myweb.demos.api.model.ImageMetadataTags">
        update image_metadata_tags
        set 
            tag = #{tag}
        where   image_id = #{imageId} AND tag = #{tag} 
    </update>
</mapper>